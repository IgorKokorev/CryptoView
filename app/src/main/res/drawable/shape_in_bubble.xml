<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <rotate
            android:fromDegrees="-35"
            android:pivotX="0%"
            android:pivotY="0%"
            android:toDegrees="0">
            <shape android:shape="rectangle">
                <solid android:color="@color/base2" />
            </shape>
        </rotate>
    </item>
    <item android:left="@dimen/medium_padding">
        <shape android:shape="rectangle">
            <solid android:color="@color/base2" />
            <corners android:radius="@dimen/medium_padding" />
        </shape>
    </item>

</layer-list>